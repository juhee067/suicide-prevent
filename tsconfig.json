{
  "compilerOptions": {
    "jsx": "preserve",
    "lib": [
      "es6",
      "dom"
    ],
    "rootDir": "src",
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es5",
    "sourceMap": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "./src"
, "middleware.ts"  ],
  "exclude": [
    "node_modules",
    "build"
  ]
}
